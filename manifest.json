{
  "manifest_version": 2,

  "name": "Social",
  "description": "Only for test",
  "version": "1.0",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/jquery.js", "js/jquery.json.js", "js/background.js"]
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["http://graph.renren.com/oauth/login_success.html?code=*"],
      "js": ["js/jquery.js", "js/content.js"]
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "storage"
  ]
}
